<UserControl x:Class="H.Skeepy.Playbox.TesterApp.IndividualsBlotter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:H.Skeepy.Playbox.TesterApp"
             xmlns:SkeepyRepository="clr-namespace:H.Skeepy.Playbox.TesterApp.AppData.SkeepyRepository"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ListBox ItemsSource="{Binding Source={x:Static SkeepyRepository:TeamsRepository.All}}" DisplayMemberPath="Name"/>
    </Grid>
</UserControl>
